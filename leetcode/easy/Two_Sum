#O(n^2) time complexity
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        for i in range(len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i] + nums[j] == target:
                    return[i,j]
                
#Hash map
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        seen = {}

        for i, num in enumerate(nums):
            complement = target - num

            if complement in seen:
                return [seen[complement], i]

            seen[num] = i

##I use a hash map to store previously seen numbers and their indices.
#For each number, I calculate the complement.
#If the complement exists in the hash map, I return the stored index and current index.
#Otherwise, I store the current number in the hash map.